{
  "devDependencies": {
    "@types/node": "^18.0.0",
    "eslint": "^8.18.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "npx pretty-quick --staged ng lint ng test",
      "pre-push": "ng build --aot true"
    }
  },
  "scripts": {
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "common-js": "^0.3.8",
    "express": "^4.18.1",
    "mrm-task-lint-staged": "^7.0.0",
    "nodemon": "^2.0.18",
    "ts-node": "^10.8.1"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
